<h1><%= t('.title') %></h1>

<table>
  <tr>
    <th><%= t('.name') %></th>
    <th><%= t('.code') %></th>
    <th><%= t('.marc') %></th>
    <th><%= t('.quantity') %></th>
    <th><%= t('.price') %></th>
    <th><%= t('.imgurl') %></th>
    <th><%= t('.model') %></th>
    <th><%= t('.imgurladd') %></th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @products.each do |product| %>
  <tr>
    <td><%= product.name %></td>
    <td><%= product.code %></td>
    <td><%= product.marc %></td>
    <td><%= product.quantity %></td>
    <td><%= product.price %></td>
    <td><%= product.imageurl %></td>
    <td><%= product.model %></td>
    <td><%= product.addedimageurl %></td>
    <td><%= link_to 'Show', product %></td>
    <td><%= link_to 'Edit', edit_product_path(product) %></td>
    <td><%= link_to 'Destroy', product, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to t('.new_product'), new_product_path %>
